    "@lwc/jest-transformer": "19.1.2",
    "babel-jest": "29.7.0",
    "jest": "29.7.0",
    "jest-config": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.6.2",
