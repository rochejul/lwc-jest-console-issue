{
  "jest.jestCommandLine": "./node_modules/.bin/jest --config=jest.config.json",
  "jest.runMode": "on-save",
  "jest.debugMode": true,
  "jest.coverageFormatter": "DefaultFormatter",
  "jest.coverageColors": {
    "covered": "rgba(9, 156, 65, 0.4)",
    "uncovered": "rgba(255,99,71, 0.2)",
    "partially-covered": "rgba(255,215,0, 0.2)"
  },
  "jestrunner.jestCommand": "./node_modules/.bin/jest",
  "jestrunner.configPath": "./jest.config.json",
  "jestrunner.runOptions": ["--no-cache", "--coverage=false"],
  "jestrunner.debugOptions": {
    "args": [
      "--runInBand",
      "--no-cache",
      "--coverage=false",
      "--colors",
      "--verbose",
      "--watchAll=false"
    ]
  }
}
